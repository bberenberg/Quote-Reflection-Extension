{
  "name": "Untitle Quote Extensions",
  "description": "Prompts the user with a quote and asks them to consider and then respond to the quote.",
  "version": "0.3",
  "manifest_version": 3,
  "incognito": "split",
  "chrome_url_overrides": {
      "newtab": "body.html"
  },
  "permissions": ["activeTab"],
  "host_permissions": [
    "https://boris-quoteResponder.web.val.run/"
  ],
  "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
  }
}
